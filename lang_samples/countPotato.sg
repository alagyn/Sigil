
int main(list<string> args)
{
    count = 0;

    with open(args[0], mode="r") as f
    {
        for line in f.lines()
        {
            if line.startsWith('potato')
            {
                ++count;
            }
        }
    }

    print("There are {count} potatos in {args[0]}");

    return 0;
}
